{
  "database": {
    "rules": "database.rules.json"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": ".output/server",
      "runtime": "nodejs18"
    }
  ],
  "hosting": {
    "site": "imperial-8ea0c",
